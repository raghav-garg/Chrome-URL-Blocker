{
  "name": "url-blocker",
  "version": "1.0",
  "description": "Chrome URL Blocking Extension",
  "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"],
  "background": {
    "scripts": ["main.js"]
  },
  "browser_action": {
	  "default_icon": "icon.png"
  },
  "icons": {
	   "16": "icon16.png",
	   "48": "icon48.png"
	 },
  "manifest_version": 2
}
